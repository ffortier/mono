{
    "files.associations": {
        "*.pl": "prolog",
        "*.asm": "nasm",
        "*.fs": "fsharp",
    },
    "rust-analyzer.workspace.discoverConfig": {
        "command": [
            "${workspaceFolder}/third_party/rust/discover_bazel_rust_project.sh",
            "{arg}"
        ],
        "progressLabel": "rust_analyzer",
        "filesToWatch": [
            "BUILD",
            "BUILD.bazel",
            "MODULE.bazel"
        ]
    },
    "rust-analyzer.workspace.symbol.search.scope": "workspace_and_dependencies",
    "cmake.ignoreCMakeListsMissing": true,
    "java.project.referencedLibraries": [
        "external/rules_jvm_external++maven+*/file/**/*.jar"
    ],
    "java.project.sourcePaths": [
        "experiments/jasm/examples/adder/src/main/java",
        "experiments/jasm/src/main/java",
        "experiments/jasm/src/test/java"
    ],
    "deno.enable": true,
    "nasm.outputFormat": "elf32",
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "C_Cpp.clang_format_style": "file"
}