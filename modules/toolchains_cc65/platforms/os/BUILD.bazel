load("@toolchains_cc65//platforms:platforms.bzl", "SUPPORTED_PLATFORMS")

package(default_visibility = ["//visibility:public"])

alias(
    name = "os",
    actual = "@platforms//os",
)

alias(
    name = "none",
    actual = "@platforms//os:none",
)

[
    constraint_value(
        name = os,
        constraint_setting = ":os",
    )
    for os, _ in SUPPORTED_PLATFORMS
    if os != "none"
]
