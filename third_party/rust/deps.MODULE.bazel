crate = use_extension("@rules_rust//crate_universe:extension.bzl", "crate")

crate.spec(
    package = "clap",
    version = "=4.5.28",
    features = ["derive"],
)

crate.spec(
    package = "reqwest",
    version = "=0.12.12",
    default_features = False,
    features = ["blocking", "rustls-tls", "http2", "charset"],
)

crate.spec(
    package = "anyhow",
    version = "=1.0.95",
)

crate.spec(
    package = "sha2",
    version = "=0.10.8",
)

crate.spec(
    package = "hex",
    version = "=0.4.3",
)

crate.spec(
    package = "thiserror",
    version = "=2.0.11",
)

crate.spec(
    package = "termion",
    version = "=4.0.3",
)

crate.spec(
    package = "rustls",
    version = "=0.23.22",
    default_features = False,
    features = ["logging", "tls12"],
)

crate.spec(
    package = "byteorder",
    version = "=1.5.0",
)

crate.spec(
    package = "flate2",
    version = "=1.0.35",
)

crate.from_specs()

use_repo(crate, "crates")
